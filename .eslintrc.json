{
  "env": {
    "browser": true,
    "es6": true,
    "es2017": true
  },
  "extends": ["eslint:recommended", "plugin:sonarjs/recommended"],
  "plugins": ["@typescript-eslint", "sonarjs"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "prefer-const": "error",
    "no-console": "error",
    "sonarjs/cognitive-complexity": "error"
  }
}
